{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabled": false
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "disabled": false
    },
    "supabase-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "YOUR_SUPABASE_TOKEN_HERE"
      ],
      "disabled": false
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "env": {
        "SystemRoot": "C:\\Windows",
        "PROGRAMFILES": "C:\\Program Files"
      },
      "disabled": true
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:\\BMAD-workflow",
        "C:\\Users\\vinil\\OneDrive\\√Årea de Trabalho"
      ]
    },
    "deepwiki": {
      "serverUrl": "https://mcp.deepwiki.com/sse"
    },
    "debugger": {
      "command": "node",
      "args": [
        "C:\\BMAD-workflow\\debugger-mcp\\dist\\index.js"
      ],
      "env": {}
    },
    "github-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      }
    },
    "render": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.render.com/mcp",
        "--header",
        "Authorization: Bearer YOUR_RENDER_TOKEN_HERE"
      ],
      "env": {
        "RENDER_API_KEY": "YOUR_RENDER_TOKEN_HERE"
      }
    },
    "android": {
      "command": "C:\\Users\\vinil\\AppData\\Local\\Packages\\PythonSoftwareFoundation.Python.3.13_qbz5n2kfra8p0\\LocalCache\\local-packages\\Python313\\Scripts\\uv.exe",
      "args": [
        "--directory",
        "C:\\BMAD-workflow\\android-mcp-server",
        "run",
        "server.py"
      ]
    }
  }
}