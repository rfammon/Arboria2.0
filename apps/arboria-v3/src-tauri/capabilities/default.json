{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "log:default",
    "opener:default",
    "shell:default",
    "dialog:default",
    "app-save-download-file",
    "allow-show-in-folder",
    "allow-open-file-natively",
    "allow-delete-file"
  ]
}